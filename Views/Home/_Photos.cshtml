@model List<WalkPhoto>

<div id="walk-carousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        @for (var i = 0; i < Model.Count(); i++)
        {
            <li data-target="#walk-carousel" data-slide-to="@i"></li>
        }
    </ol>
    <div class="carousel-inner">
        @for (var i = 0; i < Model.Count(); i++)
        {
            <div class="carousel-item @(i == 0 ? "active" : "")">
                <img src="@Model[i].Url" class="d-block w-100" alt="...">
            </div>
        }
    </div>
    <a class="carousel-control-prev" href="#walk-carousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#walk-carousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
