@model List<WalkPhoto>

<div id="walk-carousel" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-indicators">
        @for (var i = 0; i < Model.Count(); i++)
        {
            <button type="button" data-bs-target="#walk-carousel" data-bs-slide-to="@i" class="@(i == 0 ? "active" : "")" aria-current="@(i == 0 ? "true" : "")" aria-label="Slide 1"></button>
        }
    </div>
    <div class="carousel-inner">
        @for (var i = 0; i < Model.Count(); i++)
        {
            <div class="carousel-item @(i == 0 ? "active" : "")">
                <img src="@Model[i].Url" class="d-block w-100" alt="...">
            </div>
        }
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#walk-carousel" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
    <button class="carousel-control-next" type="button" data-bs-target="#walk-carousel" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>    
</div>
